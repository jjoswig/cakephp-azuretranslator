{
    "name": "jjoswig/azure-translator",
    "description": "AzureTranslator plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-3.0-only",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AzureTranslator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AzureTranslator\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
